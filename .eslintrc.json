{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-throw-literal": "off",
        "max-len": [
            "warn",
            120
        ]
    }
}
